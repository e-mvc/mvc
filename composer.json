{
    "name": "e-mvc/e-mvc",
    "type": "project",
    "description": "The MVC Framework.",
    "keywords": ["mvc", "php"],
    "license": "MIT",
    "require": {
        "php": "^7.0|^8.0",
    },
  "repositories": [
    {
       "type": "vcs",
